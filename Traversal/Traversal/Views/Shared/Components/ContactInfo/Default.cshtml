@model ContactInfo

<div class="partners">
    <div class="cont-details">
        <h5>Get in touch</h5>
        <p class="mt-3 mb-4">@Model.Description</p>
    </div>
    <div class="hours">
        <h6 class="mt-4">Email:</h6>
        <p>
            <a href="mailto:@Model.Email">@Model.Email</a>
        </p>
        <h6 class="mt-4">Visit Us:</h6>
        <p>@Model.Address</p>
        <h6 class="mt-4">Contact:</h6>
        <p class="margin-top"><a href="tel:@Model.PhoneNumber">@Model.PhoneNumber</a></p>
    </div>
</div>